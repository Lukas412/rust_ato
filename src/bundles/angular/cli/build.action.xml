<?xml version="1.0" encoding="UTF-8" ?>

<action_pack xmlns="" namespace="angular::cli::build">

  <any-parameters>
    <string_parameter name="flags" namespace="angular::cli::build::*"/>
  </any-parameters>
  <location_change>
    <path_get-argument namespace="angular::project" name="name"/>
    <action_reference namespace="ato::command::invoke">
      <string_argument name="command" value="ng build"/>
      <string_argument name="flags">
        <string_get-argument namespace="angular::cli::build" name="flags"/>
      </string_argument>
    </action_reference>
  </location_change>
</action_pack>