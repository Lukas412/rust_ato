<?xml version="1.0" encoding="UTF-8" ?>

<string_pack xmlns="" namespace="angular::cli::init::style">

  <element_parameters>
    <string_parameter name="type"/>
  </element_parameters>
  <string_switch>
    <value>
      <string_get-argument namespace="angular::cli::init::style" name="type"/>
    </value>
    <case value="css" then="css"/>
    <case value="scss" then="scss"/>
    <case value="sass" then="sass"/>
  </string_switch>
</string_pack>