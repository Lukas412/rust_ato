<?xml version="1.0" encoding="UTF-8" ?>

<string:pack xmlns:string="http://www.ato.net/xmlns/element/string"
             namespace="angular::cli::init::style">

  <parameters>
    <string:parameter name="type"/>
  </parameters>
  <string:switch>
    <value>
      <string:get_argument namespace="angular::cli::init::style" name="type"/>
    </value>
    <case value="css" then="css"/>
    <case value="scss" then="scss"/>
    <case value="sass" then="sass"/>
  </string:switch>
</string:pack>