<?xml version="1.0" encoding="UTF-8" ?>

<action_pack xmlns="" namespace="git::commit">

  <any-parameters>
    <string_parameter name="flags" namespace="git::commit::*"/>
  </any-parameters>
  <action_reference namespace="ato::command::invoke">
    <string_argument name="command" value="git commit"/>
    <string_argument name="flags">
      <string_get-argument namespace="git::commit" name="flags"/>
    </string_argument>
  </action_reference>
</action_pack>